@if (ViewBag.Message != null)
{
    if (!ViewBag.Message.Equals("Nothing to generate"))
    {
<script>
            $.toast({
                heading: 'Success',
                text: "@ViewBag.Message",
                icon: 'success'
            });
</script>
    }
    else
    {
<script>
            $.toast({
                heading: 'Failed',
                text: "@ViewBag.Message",
                icon: 'fail'
            });
</script>
    }
}
